{
  "hosting": {
    "source": "**/*.@(ico|png|jpg|jpeg|gif|js|css)",
    "headers": [
      {
        "key": "Cache-Control",
        "value": "public, max-age=300, s-maxage=600"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "europe-west1"
    }
  }
}
